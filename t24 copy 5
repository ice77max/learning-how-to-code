from turtle import *

# setup

speed(0)
tracer(50)
pensize(2)

def move_diagnal(lenght):
    for i in range(lenght):
        left(90)
        fd(1)
        right(90)
        fd(1)

for i in range(30):
    move_diagnal(100)
    left(90)
    circle(100,180)
    move_diagnal(100)
    left(6)

home()
pencolor("black")
dot(200)
pencolor("white")
dot(196)



exitonclick()